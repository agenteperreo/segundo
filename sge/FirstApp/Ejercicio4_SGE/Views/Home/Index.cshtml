<style>
    table {
        border-collapse: collapse;
    }

    td {
        border: 1px solid black;
        padding: 5px;
        width: 400px;
        text-align: center;
    }
</style>

<h1>Tabla de multiplicar de 1 al 12</h1>
<table>
    @for (int i = 1; i <= 12; i++)
    {
        <tr>
            @for (int d = 1; d <= 10; d++)
            {
                <td>@i x @d = @(i*d)</td>
            }
        </tr>
    }
</table>

    

